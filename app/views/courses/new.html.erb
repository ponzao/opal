<h1>New course</h1>

<% form_for(@course) do |f| %>
    <%= f.error_messages %>

    <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
    </p>
    <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows => 6 %>
    </p>
    <p>
    <%= f.submit "new" %>
    </p>
<% end %>

<%= link_to "Back", "courses" %>
