<h2>Edit form</h2>
<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
  <%= f.label :username %>
  <%= f.text_field :username %>
  </p>

  <p>
  <%= f.label :email %>
  <%= f.text_field :email %>
  </p>
  
  <p>
  <%= f.label :student_number %>
  <%= f.text_field :student_number %>
  </p>

  <p>
    <%= f.submit "Change" %>
  </p>
<% end %>
