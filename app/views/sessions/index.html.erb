<h2>Login form</h2>
<% form_for @user, :url => sessions_url do |f| %>
  <%= f.error_messages %>

  <p>
  <%= f.label :username %>
  <%= f.text_field :username %>
  </p>

  <p>
  <%= f.label :password %>
  <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Login" %>
  </p>
<% end %>

<p><%= link_to "Register?", new_user_url %></p>
